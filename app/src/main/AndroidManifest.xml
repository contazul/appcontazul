<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.appcontazul">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/contazullogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/contazullogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CadastroActivity" />
        <activity android:name=".activity.SelecaoContaActivity" />
        <activity android:name=".activity.CentralActivity" />
        <activity android:name=".activity.PerfilContaActivity" />
        <activity android:name=".activity.MenuActivity" />
        <activity android:name=".activity.SomaDeSaldoActivity" />
        <activity android:name=".activity.SubtracaoDeSaldoActivity" />
        <activity android:name=".activity.LucroMensalActivity"></activity>
    </application>

</manifest>